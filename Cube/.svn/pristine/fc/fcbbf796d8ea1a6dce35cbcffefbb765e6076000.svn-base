#include "Unit.h"



Unit::Unit()
{
}
Unit::Unit(int ID,Sprite_Sheet_Data base, sf::Vector2f position, World<Physical> *mapa, float speed) : Moving(ID, base, position, mapa, speed)
{

}


Unit::~Unit()
{
}

void Unit::update(World<Physical>*mapa)
{
	this->Movement();
	this->Border_Collision(mapa);
	this->Physical_Collision(mapa);
	this->Animate();
	mapa->Set_obstacle(this->ID, this->getPosition().x, this->getPosition().y);
	
	
}
